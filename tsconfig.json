{
    "compilerOptions": {
        "outDir": "./built/",
        "sourceMap": true,
        "strict": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es3",
        "experimentalDecorators": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "strictNullChecks" : false,
        "noImplicitAny" : false 
    },
    "include": [
        "./src/**/*",
        "./src/App/**/*"
    ]
}